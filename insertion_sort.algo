

// tri par insertion
ROCEDURE insertion_sort(VAR arr : ARRAY_OF INTEGER)
var
   i,j,n : INTEGER;
   x : INTEGER;
begin 
   
   for i FROM 0 TO n-1 STEP 1 do
       x:=arr[i]
       j=i
      while(j>=0 AND arr[j-1]>x)
         arr[j]:=arr[j-1]
         j=j-1
      end while
      arr[j]:=x
      
    end for
end
